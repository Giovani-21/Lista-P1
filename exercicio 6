#include <stdio.h>

int main(void) {


float p_resistividade;
float A_치rea;
float L_comprimento;
float R_resistencia;


// entrada com os valores


printf("p_resistividade[p]:\n");
scanf("%f",&p_resistividade);

printf("A_치rea[A]:\n");
scanf("%f",&A_치rea);

printf("L_comprimento[L]:\n");
scanf("%f",&L_comprimento);

// calcular 

printf("inicio: Calculo\n");
R_resistencia= p_resistividade * L_comprimento/A_치rea;

//saida da resistencia total
printf("o valor total da R_resistencia:%f", R_resistencia);
;



  
  return 0;
}
